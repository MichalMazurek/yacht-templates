[
    {
        "type": 1,
        "title": "HEIMDALL",
        "name": "heimdall",
        "description": "As the name suggests Heimdall Application Dashboard is a dashboard for all your web applications. It doesn't need to be limited to applications though, you can add links to anything you like.",
        "logo": "https://github.com/MichalMazurek/yacht-templates/blob/main/logos/heimdall.png?raw=true",
        "image": "lscr.io/linuxserver/heimdall:latest",
        "categories": [
            "Dashboard"
        ],
        "platform": "linux",
        "restart_policy": "unless-stopped",
        "ports": [
            {
                "WebUI": "80:80/tcp",
                "HTTPS WebUI": "443:443/tcp"
            }
        ],
        "volumes": [
            {
                "container": "/config",
                "bind": "!config/heimdall"
            }
        ],
        "env": [
            {
                "name": "PUID",
                "label": "PUID",
                "default": "!PUID"
            },
            {
                "name": "PGID",
                "label": "PGID",
                "default": "!PGID"
            },
            {
                "name": "TZ",
                "label": "TZ",
                "description": "Time zone",
                "default": "Europe/London"
            }
        ]
    }
]
